        title = "{LSQ13ddu: a rapidly evolving stripped-envelope supernova with early circumstellar interaction signatures}",
      journal = {\mnras},
     keywords = {circumstellar matter, supernovae: general, supernovae: individual: LSQ13ddu, Astrophysics - High Energy Astrophysical Phenomena, Astrophysics - Solar and Stellar Astrophysics},
         year = 2020,
        month = feb,
       volume = {492},
       number = {2},
        pages = {2208-2228},
     abstract = "{This paper describes the rapidly evolving and unusual supernova
        LSQ13ddu, discovered by the La Silla-QUEST survey. LSQ13ddu
        displayed a rapid rise of just 4.8 {\ensuremath{\pm}} 0.9 d to
        reach a peak brightness of -19.70 {\ensuremath{\pm}} 0.02 mag in
        the LSQgr band. Early spectra of LSQ13ddu showed the presence of
        weak and narrow He I features arising from interaction with
        circumstellar material (CSM). These interaction signatures
        weakened quickly, with broad features consistent with those seen
        in stripped-envelope SNe becoming dominant around two weeks
        after maximum. The narrow He I velocities are consistent with
        the wind velocities of luminous blue variables but its spectra
        lack the typically seen hydrogen features. The fast and bright
        early light curve is inconsistent with radioactive $^{56}$Ni
        powering but can be explained through a combination of CSM
        interaction and an underlying $^{56}$Ni decay component that
        dominates the later time behaviour of LSQ13ddu. Based on the
        strength of the underlying broad features, LSQ13ddu appears
        deficient in He compared to standard SNe Ib.}",
          doi = {10.1093/mnras/stz3598},
archivePrefix = {arXiv},
       eprint = {1912.05986},
 primaryClass = {astro-ph.HE},
       adsurl = {https://ui.adsabs.harvard.edu/abs/2020MNRAS.492.2208C},
      adsnote = {Provided by the SAO/NASA Astrophysics Data System}
}

@ARTICLE{2020A&A...634A..21S,
       author = {{Stritzinger}, M.~D. and {Taddia}, F. and {Holmbo}, S. and {Baron}, E. and {Contreras}, C. and {Karamehmetoglu}, E. and {Phillips}, M.~M. and {Sollerman}, J. and {Suntzeff}, N.~B. and {Vinko}, J. and {Ashall}, C. and {Avila}, C. and {Burns}, C.~R. and {Campillay}, A. and {Castellon}, S. and {Folatelli}, G. and {Galbany}, L. and {Hoeflich}, P. and {Hsiao}, E.~Y. and {Marion}, G.~H. and {Morrell}, N. and {Wheeler}, J.~C.},
        title = "{The Carnegie Supernova Project II. Early observations and progenitor constraints of the Type Ib supernova LSQ13abf}",
